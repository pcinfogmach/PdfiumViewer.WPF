<Application
    x:Class="PdfiumViewer.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:PdfiumViewer.Demo.Converters"
    xmlns:local="clr-namespace:PdfiumViewer.Demo"
    StartupUri="MainWindow.xaml">
    <Application.Resources>

        <converters:IconToImageSourceConverter x:Key="IconToImageSourceConverter" />
        <converters:MathConverter x:Key="MathConverter" />
        <converters:StringToIntConverter x:Key="StringToIntConverter" />
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    </Application.Resources>
</Application>
